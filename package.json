{
	"name": "mc-datapack",
	"displayName": "mc-datapack",
	"description": "Makes the creation of Minecraft datapacks easier.",
	"icon": "images/icon.png",
	"version": "1.0.0",
	"publisher": "HuJohner",
	"engines": {
		"vscode": "^1.44.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mc-datapack.newDatapack"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mc-datapack.newDatapack",
				"title": "New datapack",
				"category": "Datapack"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "mc-datapack.newDatapack",
					"group": "mc-datapack"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
